{% extends 'base.html.twig' %}

{% block title %}
    {{ parent() }}
    The best cars in the internet!
{% endblock %}

{% block body %}
    <h1>Our Offer</h1>
    <table class="table">
        <tr>
            <th>Mark</th>
            <th>Name</th>
        </tr>
        {% for car in cars %}
            <tr>
                <td>{{ car.mark }}</td>
                <td>{{ car.name }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}